{
    "plugins": ["jsdoc-plugin-typescript"],
    "source": {
        "include": ["dist"],
        "includePattern": "(account|api|(boolean|numeric)_filter|collection_helpers|config|enums|index|obj_with_counts_list|toot|tag_list|types|user_data)\\.[jt]sx?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "encoding": "utf8",
        "recurse": true,
        "template": "./jsdoc_template"
    },
    "templates": {
        "default": {
            "outputSourceFiles": false
        }
    }
}
