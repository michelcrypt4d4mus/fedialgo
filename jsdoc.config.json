{
    "plugins": ["jsdoc-plugin-typescript"],
    "source": {
        "include": ["dist"],
        "includePattern": "(account|config|enums|index|toot|tag_list|types|user_data|(boolean|numeric)_filter)\\.[jt]sx?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true
    }
}
