{
    "plugins": ["jsdoc-plugin-typescript"],
    "source": {
        "include": ["dist"],
        "includePattern": "(account|api|collection_helpers|config|enums|index|toot|tag_list|types|user_data|(boolean|numeric)_filter)\\.[jt]sx?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "encoding": "utf8",
        "recurse": true,
        "template": "./jsdoc_template"
    }
}
