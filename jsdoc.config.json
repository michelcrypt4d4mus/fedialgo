{
    "plugins": ["jsdoc-plugin-typescript"],
    "source": {
        "include": ["dist"],
        "includePattern": "(account|api|(boolean|numeric|toot)_filter|collection_helpers|config|enums|index|toot|types|user_data|.*_list)\\.[jt]sx?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs",
        "encoding": "utf8",
        "recurse": true,
        "template": "./jsdoc_template"
    },
    "templates": {
        "default": {
            "outputSourceFiles": false
        }
    }
}
